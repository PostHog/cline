    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": { "foreground": "#ffffff80" }
        },
        { "name": "Strings", "scope": ["string", "constant.other.symbol"], "settings": { "foreground": "#5f9d32" } },
        {
            "name": "Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": { "foreground": "#df4b20" }
        },
        {
            "name": "Variables",
            "scope": ["variable", "variable.language", "variable.parameter"],
            "settings": { "foreground": "#fff" }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function", "meta.function-call", "support.function"],
            "settings": { "foreground": "#f7a503" }
        },
        {
            "name": "Classes",
            "scope": ["entity.name.type", "entity.name.class", "entity.name.namespace", "entity.name.scope-resolution"],
            "settings": { "foreground": "#747ea1" }
        },
        {
            "name": "Numbers",
            "scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
            "settings": { "foreground": "#e49f2c" }
        },
        { "name": "Operators", "scope": "keyword.operator", "settings": { "foreground": "#ffa599" } },
        {
            "name": "Markup Headings",
            "scope": ["markup.heading", "entity.name.section"],
            "settings": { "foreground": "#df4b20", "fontStyle": "bold" }
        },
        { "name": "Markup Bold", "scope": "markup.bold", "settings": { "foreground": "#f7a503", "fontStyle": "bold" } },
        {
            "name": "Markup Italic",
            "scope": "markup.italic",
            "settings": { "foreground": "#f7a503", "fontStyle": "italic" }
        },
        {
            "name": "Markup Links",
            "scope": ["markup.underline.link", "string.other.link"],
            "settings": { "foreground": "#747ea1" }
        }
    ],
    "semanticHighlighting": true
}
